<?xml version="1.0" encoding="utf-8"?>
<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:drawable="@drawable/vector_morphing_menu">

    <target
        android:animation="@animator/animation_menu_to_arrow_rectangle_1"
        android:name="rectangle_1"/>
    
    <target
        android:animation="@animator/animation_menu_to_arrow_rectangle_2"
        android:name="rectangle_2"/>
    
    <target
        android:animation="@animator/animation_menu_to_arrow_rectangle_3"
        android:name="rectangle_3"/>
    
    <target
        android:animation="@animator/animation_menu_point_1_to_arrow_1"
        android:name="rectangle_path_1"/>

    <target
        android:animation="@animator/animation_menu_point_2_to_arrow_2"
        android:name="rectangle_path_2"/>

    <target
        android:animation="@animator/animation_menu_point_3_to_arrow_3"
        android:name="rectangle_path_3"/>

</animated-vector>