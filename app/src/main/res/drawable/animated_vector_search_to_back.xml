<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:drawable="@drawable/vector_morphing_search_to_back">
    
    <target
        android:name="stem"
        android:animation="@animator/animation_search_to_back_stem"/>
    <target
        android:name="search_circle"
        android:animation="@animator/animation_search_to_back_circle"
        />
    <target
        android:name="arrow_head"
        android:animation="@animator/animation_search_to_back_arrow"
        />
    <target
        android:animation="@animator/animation_search_to_back_arrow_top"
        android:name="arrow_head_top"/>
    <target
        android:animation="@animator/animation_search_to_back_arrow_bottom"
        android:name="arrow_head_bottom"/>
    
</animated-vector>