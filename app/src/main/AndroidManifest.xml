<manifest package="com.zero.amazinganim"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>

        <activity android:name=".TransformingPathActivity"></activity>
        <activity android:name=".TrimmingPathActivity"></activity>
        <activity android:name=".DrawPathActivity"></activity>
        
        <activity android:name=".MorphingActivity"></activity>
        <activity android:name=".MorphingNumActivity"></activity>
        <activity android:name=".MorphingTickActivity"></activity>
        <activity android:name=".MorphingProgressBarActivity"></activity>
        <activity android:name=".MorphingFingerActivity"></activity>
        <activity android:name=".MorphingSearchToBackActivity"></activity>
        <activity android:name=".MorphingMenuActivity"></activity>
        <activity android:name=".MorphingHandwritingActivity"></activity>
        
    </application>

</manifest>
